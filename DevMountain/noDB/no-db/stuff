removeFav: (req, res) => {
    const { id } = req.params;
    const newFavList = favList.filter(fav => {
      if (fav.id !== +id) return fav;
    });
    favList = [...newFavList] 
    res.status(200).send(favList);
  },




 deleteStar = (id) => {
        axios.delete(`/api/favList/${id}`)
        .then(res => {
          this.props.deleteFn(res.data)
        })
        .catch(err => console.log(err));




export default App